testcases = int(input())
members = int(input())
g_revolution = [int(x) for x in input().split() for i in range(1)]
all_starz = [int(x) for x in input().split() for i in range(1)]
count = 0

for i in range(members):
    res1 = max(g_revolution)-max(all_starz)
    if(res1>0):
        count = count + 1
        g_revolution.remove(max(g_revolution))
        all_starz.remove(max(all_starz))
print(count)
